{
  "name": "symposium",
  "displayName": "Symposium",
  "description": "AI the Rust Way",
  "version": "1.3.0",
  "publisher": "symposium-dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/symposium/symposium.git"
  },
  "license": "MIT",
  "icon": "resources/extension-icon.png",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languageModelChatProviders": [
      {
        "vendor": "symposium",
        "displayName": "Symposium"
      }
    ],
    "languageModelTools": [
      {
        "name": "symposium-agent-action",
        "toolReferenceName": "symposiumAction",
        "displayName": "Agent Action",
        "modelDescription": "Request permission for an agent to execute an internal tool",
        "userDescription": "Allow the agent to execute a tool",
        "canBeReferencedInPrompt": true,
        "inputSchema": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string",
              "description": "Human-readable title for the action"
            },
            "kind": {
              "type": "string",
              "description": "The kind of tool (e.g., bash, file_edit)"
            },
            "raw_input": {
              "type": "object",
              "description": "The raw input parameters for the tool"
            }
          }
        }
      }
    ],
    "configuration": {
      "title": "Symposium",
      "properties": {
        "symposium.logLevel": {
          "type": "string",
          "enum": [
            "error",
            "info",
            "debug"
          ],
          "default": "error",
          "description": "Log verbosity level. 'error' shows only errors and agent connections, 'info' adds general activity, 'debug' shows everything."
        },
        "symposium.agents": {
          "type": "object",
          "patternProperties": {
            ".*": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "description": "Display name for the agent"
                },
                "version": {
                  "type": "string",
                  "description": "Version of the agent"
                },
                "description": {
                  "type": "string",
                  "description": "Description of the agent"
                },
                "distribution": {
                  "type": "object",
                  "description": "Distribution methods for spawning the agent",
                  "properties": {
                    "npx": {
                      "type": "object",
                      "properties": {
                        "package": {
                          "type": "string",
                          "description": "NPM package name"
                        },
                        "args": {
                          "type": "array",
                          "items": {
                            "type": "string"
                          },
                          "description": "Additional arguments"
                        }
                      },
                      "required": [
                        "package"
                      ]
                    },
                    "pipx": {
                      "type": "object",
                      "properties": {
                        "package": {
                          "type": "string",
                          "description": "PyPI package name"
                        },
                        "args": {
                          "type": "array",
                          "items": {
                            "type": "string"
                          },
                          "description": "Additional arguments"
                        }
                      },
                      "required": [
                        "package"
                      ]
                    },
                    "binary": {
                      "type": "object",
                      "description": "Binary distributions keyed by platform (e.g., darwin-aarch64, linux-x86_64)",
                      "additionalProperties": {
                        "type": "object",
                        "properties": {
                          "archive": {
                            "type": "string",
                            "description": "Download URL for the binary archive"
                          },
                          "cmd": {
                            "type": "string",
                            "description": "Command to run (e.g., ./opencode)"
                          },
                          "args": {
                            "type": "array",
                            "items": {
                              "type": "string"
                            },
                            "description": "Additional arguments"
                          }
                        },
                        "required": [
                          "archive",
                          "cmd"
                        ]
                      }
                    }
                  }
                },
                "_source": {
                  "type": "string",
                  "enum": [
                    "registry",
                    "custom"
                  ],
                  "description": "Source of the agent configuration (registry = auto-updated, custom = user-defined)"
                }
              },
              "required": [
                "distribution"
              ]
            }
          },
          "default": {},
          "description": "Additional ACP agents (keyed by agent ID). Built-in agents (zed-claude-code, zed-codex, google-gemini) are always available."
        },
        "symposium.currentAgentId": {
          "type": "string",
          "default": "zed-claude-code",
          "description": "ID of the currently selected agent"
        },
        "symposium.traceDir": {
          "type": "string",
          "default": "",
          "description": "Directory to write trace files for debugging. Leave empty to disable tracing. Traces can be viewed with sacp-trace-viewer."
        },
        "symposium.agentLogLevel": {
          "type": "string",
          "enum": [
            "",
            "error",
            "warn",
            "info",
            "debug",
            "trace"
          ],
          "default": "",
          "description": "Log level for the symposium-acp-agent process. Logs appear in the Symposium output channel. Leave empty to disable."
        },
        "symposium.requireModifierToSend": {
          "type": "boolean",
          "default": false,
          "description": "When enabled, Enter adds a newline and Shift+Enter (or Cmd+Enter on Mac) sends the prompt. When disabled (default), Enter sends and Shift+Enter adds a newline."
        },
        "symposium.bypassPermissions": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "List of agent IDs that automatically approve all tool execution requests."
        },
        "symposium.enableExperimentalLM": {
          "type": "boolean",
          "default": false,
          "description": "Enable experimental Language Model provider integration. This allows other VS Code extensions to use Symposium agents via the Language Model API. Requires restart."
        },
        "symposium.extensions": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Extension identifier (e.g., sparkle, ferris, cargo)"
              },
              "enabled": {
                "type": "boolean",
                "description": "Whether this extension is enabled"
              }
            },
            "required": [
              "id",
              "enabled"
            ]
          },
          "default": [
            {
              "id": "sparkle",
              "enabled": true
            },
            {
              "id": "ferris",
              "enabled": true
            },
            {
              "id": "cargo",
              "enabled": true
            }
          ],
          "description": "Agent extensions (proxies) to include in the Symposium chain. Order matters - extensions are applied in the order listed."
        },
        "symposium.agentSpawnArgs": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "Additional args to pass to `symposium-acp-agent`"
        }
        ,
        "symposium.acpAgentPath": {
          "type": "string",
          "default": "",
          "description": "Path to the `symposium-acp-agent` binary to use. If set, this overrides the bundled binary and PATH lookup. Leave empty to use bundled or PATH resolution."
        }
      }
    },
    "commands": [
      {
        "command": "symposium.openChat",
        "title": "Open Symposium Chat"
      },
      {
        "command": "symposium.discussSelection",
        "title": "Discuss in Symposium"
      },
      {
        "command": "symposium.addAgentFromRegistry",
        "title": "Symposium: Add Agent from Registry"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "symposium",
          "title": "Symposium",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "symposium": [
        {
          "type": "webview",
          "id": "symposium.chatView",
          "name": "Chat"
        },
        {
          "type": "webview",
          "id": "symposium.settingsView",
          "name": "Settings"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run webpack",
    "webpack": "webpack --mode production",
    "webpack-dev": "webpack --mode development",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "test": "npm run compile && vscode-test"
  },
  "devDependencies": {
    "@types/node": "^20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.4.0",
    "typescript": "^5.4.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {
    "@agentclientprotocol/sdk": "^0.5.1",
    "@aws/mynah-ui": "file:../vendor/mynah-ui",
    "@types/uuid": "^10.0.0",
    "uuid": "^13.0.0"
  }
}
